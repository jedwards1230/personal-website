{
	"name": "personal-website",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbo",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write . --ignore-path .gitignore",
		"pages:build": "npx @cloudflare/next-on-pages",
